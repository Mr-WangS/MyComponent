.toast-content-box{
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
  position: fixed;
  z-index: 333;
}
.toast-content{
  /* width: 50%; */
  padding:15rpx 30rpx;
  background: #4D4D4D;
  border-radius: 10rpx;
  
}
.toast-content-text{
  height: 100%;
  width: 100%;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
}